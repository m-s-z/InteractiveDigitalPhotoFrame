@model WebApplication.ViewModels.RegisterConfirmViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
@if (Session["UserId"] != null)
{
    Response.Redirect("~/Home");
}
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Register</title>
    <link runat="server" rel="shortcut icon" href="~/Images/Logo_web.ico" type="image/x-icon" />
    <link runat="server" rel="icon" href="~/Images/Logo_web.ico" type="image/ico" />
    @Styles.Render("~/Content/css")
</head>
<body>
    <div>
        <div class="background-transparent">
            <h2 class="text-center">@Model.Message</h2>
            <div class="text-center">
                @Html.ActionLink("Back", "Index", "Login", new { area = "" }, new { @class = "btn btn-primary" })
            </div>
        </div>
    </div>
</body>
</html>
